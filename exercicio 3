from random import randint 

m = int(input('digite o tamanho da matriz'))

m2 = m 

mat=[]
for i in range (m):
    linha = []
    for j in range( m):
         n = randint(1,9)
         linha.append(n)
    mat.append(linha) 

soma = 0
quantidade = 0

for linha in mat:
     for n in linha:
          soma += n
          quantidade+= 1
media = soma/quantidade
print (media)   
print( soma)

for i in range (m):


     print (mat[i])
