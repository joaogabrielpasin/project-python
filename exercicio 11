l = []

d = int(input('digite o numero que deseja tirar'))
while True:

    n = int(input('digite um numero'))

    r = str(input('Deseja continuar: [S/N]'))


    if n not in l :

        l.append(n)

    if d in l :

        l.remove(d)

        print('este numero nao pode aparecer na lista:',d)

    else :

        print('valor duplicado')

    
    if r in 'Nn':

        break
